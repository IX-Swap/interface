  "scripts": {
    "start": "NODE_ENV=development webpack serve --color --progress",
    "build": "NODE_ENV=production webpack --color --progress",
    "test:e2e": "playwright test __tests__/tests --project chrome",
    "test:smoke": "playwright test __tests__/tests/identities.spec.ts ",
    "test": "jest --env=jest-environment-jsdom-sixteen --colors --silent",
    "test:watch": "jest --watch --env=jest-environment-jsdom-sixteen --coverage",
    "test:ci": "CI=true jest --env=jest-environment-jsdom-sixteen --silent --coverage --testResultsProcessor=jest-sonar-reporter --maxWorkers=75%",
    "pre-install": "rimraf node_modules && yarn cache clean",
    "lint": "eslint --cache --fix 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.tsx'",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "type-check": "tsc -p . --noEmit",
    "compile-abi-types": "typechain --target ethers-v5 --out-dir src/abis/types \"./src/abis/**/*.json\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --cache",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }

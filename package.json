  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test:watch": "react-scripts test --env=jest-environment-jsdom-sixteen --runInBand",
    "test": "CI=true react-scripts test --env=jest-environment-jsdom-sixteen --silent --runInBand",
    "test:ci": "CI=true react-scripts test --env=jest-environment-jsdom-sixteen --silent --coverage --testResultsProcessor jest-sonar-reporter --maxWorkers=75%",
    "eject": "react-scripts eject",
    "lint": "eslint --cache --fix \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.tsx\""
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!node_modules/**",
      "!src/__fixtures__/**",
      "!src/**/*.d.ts",
      "!src/**/*.styles.ts",
      "!src/**/styles.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && yarn lint",
      "pre-push": "yarn test"
    }
  },
  "babel": {
    "plugins": [
      "@fullstory/babel-plugin-annotate-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }

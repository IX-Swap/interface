  "scripts": {
    "compile-contract-types": "yarn compile-external-abi-types",
    "compile-external-abi-types": "typechain --target ethers-v5 --out-dir src/abis/types \"./src/abis/**/*.json\"",
    "build": "yarn compile-contract-types && yarn i18n:extract && yarn i18n:compile && react-app-rewired --no-warnings --optimize_for_size --max_old_space_size=6144 build && yarn sentry:sourcemaps",
    "i18n:extract": "lingui extract --locale en-US",
    "i18n:compile": "lingui compile",
    "integration-test": "cd e2e_tests && xvfb-run npx playwright test",
    "postinstall": "yarn compile-contract-types",
    "start": "set GENERATE_SOURCEMAP=true && yarn compile-contract-types && react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom",
    "serve": "serve -s build -l 3000",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org investax-ze --project $SENTRY_PROJECT_NAME ./build && sentry-cli sourcemaps upload --org investax-ze --project $SENTRY_PROJECT_NAME ./build"
  },
  "browserslist": {

  },
  "scripts": {
    "start": "react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "integration-test": "concurrently \"yarn start\" \"wait-on http-get://localhost:3000 && playwright test __tests__/tests \" --kill-others --success first",
    "eject": "react-scripts eject",
    "test": "TZ=UTC react-scripts test --env=jest-environment-jsdom-sixteen --colors --logHeapUsage --silent",
    "pre-install": "rimraf node_modules && yarn cache clean",
    "test:watch": "TZ=UTC react-scripts test --env=jest-environment-jsdom-sixteen --coverage",
    "test:gc": "TZ=UTC react-scripts --expose-gc test --env=jest-environment-jsdom-sixteen --colors --logHeapUsage --silent",
    "test:ci": "CI=true TZ=UTC node --max-old-space-size=1024 ./node_modules/react-scripts/scripts/test.js --env=jest-environment-jsdom-sixteen --silent --coverage --testResultsProcessor=jest-sonar-reporter --maxWorkers=75% --logHeapUsage",
    "lint": "eslint --cache --fix \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.tsx\"",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "resetMocks": false,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!node_modules/**",
      "!src/__fixtures__/**",
      "!src/**/*.d.ts",
      "!src/**/*.styles.ts",
      "!src/**/styles.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint",
      "yarn format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "babel": {
    "plugins": [
      "@fullstory/babel-plugin-annotate-react"
    ]
  }
}

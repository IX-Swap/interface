  "scripts": {
    "compile-contract-types": "yarn compile-external-abi-types",
    "compile-external-abi-types": "typechain --target ethers-v5 --out-dir src/abis/types \"./src/abis/**/*.json\"",
    "build": "yarn compile-contract-types && yarn i18n:extract && yarn i18n:compile && react-scripts --max_old_space_size=8192 build",
    "i18n:extract": "lingui extract --locale en-US",
    "i18n:compile": "lingui compile",
    "integration-test": "concurrently \"yarn start\" \"wait-on http-get://localhost:3000 && cd e2e_tests && xvfb-run npx playwright test --workers=1 --timeout 180000 \" --kill-others --success first",
    "postinstall": "yarn compile-contract-types",
    "start": "yarn compile-contract-types && react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "eslintConfig": {
    "extends": "react-app",
    "ignorePatterns": [
      "node_modules"
    ]
  },
